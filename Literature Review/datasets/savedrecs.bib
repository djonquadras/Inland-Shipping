@inproceedings{ WOS:000282243600004,
Author = {Burov, E.},
Editor = {Cloetingh, S and Negendank, J},
Title = {Thermo-Mechanical Models for Coupled Lithosphere-Surface Processes:
   Applications to Continental Convergence and Mountain Building Processes},
Booktitle = {NEW FRONTIERS IN INTEGRATED SOLID EARTH SCIENCES},
Series = {International Year of Planet Earth},
