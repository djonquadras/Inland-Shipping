@article{ WOS:000615495000005,
Author = {Zhu, Yuchao and Zhang, Rong-Hua and Li, Delei and Chen, Dake},
Title = {The Thermocline Biases in the Tropical North Pacific and Their
   Attributions},
Journal = {JOURNAL OF CLIMATE},
