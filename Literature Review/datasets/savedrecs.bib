@article{ WOS:000521326000002,
Author = {Wang, Kehao and Xiong, Zhenhua and Chen, Lin and Zhou, Pan and Shin,
   Hyundong},
Title = {Joint time delay and energy optimization with intelligent overclocking
   in edge computing},
Journal = {SCIENCE CHINA-INFORMATION SCIENCES},
