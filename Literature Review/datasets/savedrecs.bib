@article{ WOS:000880335200010,
Author = {Xian Jiangfeng and Wu Huafeng and Mei Xiaojun and Zhang Yuanyuan and
   Chen Xinqiang and Zhang Qiannan and Liang Linian},
Title = {Novel Energy-Efficient Opportunistic Routing Protocol for Marine
   Wireless Sensor Networks Based on Compressed Sensing and Power Control},
