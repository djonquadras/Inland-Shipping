@article{ WOS:000398063100043,
Author = {Cai, Yongqing and Wen, Yuanqiao and Wu, Lichuan and Zhou, Chunhui and
   Zhang, Fan},
Title = {Impact of wave breaking on upper-ocean turbulence},
